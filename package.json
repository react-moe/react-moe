{
  "name": "react-moe",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "dev": "react-scripts start",
    "analyze": "source-map-explorer ./build/static/js/main.*",
    "test": "react-scripts test",
    "lint": "eslint . && prettier --list-different \"./**/*.{js,json,css,md}\"",
    "format": "eslint --fix . && prettier --write \"./**/*.{js,json,css,md}\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "^1.1.0"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1",
    "prettier": "^1.10.2",
    "source-map-explorer": "^1.5.0"
  }
}
